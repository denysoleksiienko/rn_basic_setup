{
  "name": "rn_basic_setup",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "jest",
    "ts:check": "tsc --noEmit",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "start-clean": "yarn start --reset-cache",
    "clean-project": "react-native-clean-project",
    "clean:android": "cd android && ./gradlew clean && rm -rf ./build && cd ../",
    "clean:ios": "cd ios && rm -rf ./pods && rm -rf ./Podfile.lock && cd ../",
    "clean-cache": "rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf node_modules && yarn cache clean",
    "install-full": "yarn install && cd ./ios && pod update && pod install && cd ../",
    "link-font": "react-native-asset",
    "tidy": "eslint . --ext .js,.ts,.tsx --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/native": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.51.3",
    "react-native": "0.74.1",
    "react-native-bootsplash": "^5.5.3",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "^2.16.0",
    "react-native-keyboard-controller": "^1.11.7",
    "react-native-reanimated": "^3.11.0",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "^15.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.83",
    "@react-native/eslint-config": "0.74.83",
    "@react-native/metro-config": "^0.74.83",
    "@react-native/typescript-config": "0.74.83",
    "@types/lodash": "^4.17.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.6.3",
    "prettier": "^3.2.5",
    "react-native-svg-transformer": "^1.3.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
